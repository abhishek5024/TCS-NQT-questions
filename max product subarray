#include <iostream>
#include<climits>
using namespace std;

int main() {
    int arr[]={1,2,-3,0,-4,-5};
    int n=6;
    int product = 1;
    int maxi=arr[0];
    for(int i=0;i<n;i++)
    {
        product=arr[i];
        for(int j=i+1;j<n;j++)
        {
             maxi=max(maxi,product);
        product =product* arr[j];
        
    }
     maxi=max(maxi,product);
    }
    cout<<maxi;

    return 0;
}
