#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
  int arr[]={5,2,6,3,4,7,1,0};
  int n=8;
  int i =0;
  sort(arr,arr+n);
  for(int i=0;i<n;i++)
  {
    cout<<arr[i]<<" ";
  }
  cout<<endl;
  for(int i=0;i<n/2;i++)
  {
    cout<<arr[i]<<" ";
  }
  for(int i=n-1;i>=n/2;i--)
  {
    cout<<arr[i]<<" ";
  }
 
  
  return 0;
}
